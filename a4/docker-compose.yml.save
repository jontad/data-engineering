version: "3.7"
services:
  master:
   
	 build: ./master
    expose: 
      - "7077"
    ports: 
      - "9090:8080"
      - "7077:7077"

  worker:
    build: ./worker
    ports: 
      - "9091:8080"
      - "7000:7000"
    depends_on: 
      - master
    environment:
      - "MASTER=sparkmaster:7077"
      
    
